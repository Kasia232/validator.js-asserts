{
  "name": "validator.js-asserts",
  "version": "4.0.2",
  "description": "A set of extra asserts for validator.js.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Nuno Sousa",
      "email": "nunofgs@gmail.com"
    },
    {
      "name": "Rui Marinho",
      "email": "ruipmarinho@gmail.com"
    },
    {
      "name": "Tiago Ribeiro",
      "email": "tiago.ribeiro@gmail.com"
    }
  ],
  "homepage": "https://uphold.github.io/validator.js-asserts/",
  "bugs": "https://github.com/uphold/validator.js-asserts/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/uphold/validator.js-asserts"
  },
  "main": "./src/index.js",
  "keywords": [
    "validator.js",
    "asserts",
    "extra",
    "extras",
    "set"
  ],
  "options": {
    "mocha": "--recursive test"
  },
  "scripts": {
    "changelog": "github_changelog_generator --header-label='# Changelog' --future-release=v$npm_config_future_release && sed -i '' -e :a -e '$d;N;2,4ba' -e 'P;D' CHANGELOG.md",
    "cover": "node_modules/.bin/isparta cover --report html _mocha -- $npm_package_options_mocha",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha $npm_package_options_mocha",
    "version": "npm run changelog --future-release=$npm_package_version && git add -A CHANGELOG.md"
  },
  "dependencies": {
    "lodash": "^4.8.2",
    "validator.js": "^2.0.0"
  },
  "devDependencies": {
    "abavalidator": "^2.0.1",
    "bignumber.js": "^8.0.2",
    "creditcard": "^0.1.2",
    "eslint": "^6.5.1",
    "eslint-config-uphold": "^1.0.1",
    "google-libphonenumber": "^1.0.18",
    "iban": "0.0.6",
    "isoc": "0.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "moment": "^2.13.0",
    "prettier": "^1.18.2",
    "should": "^8.3.1",
    "sinon": "^1.15.4",
    "tin-validator": "^1.0.0",
    "uk-modulus-checking": "0.0.3",
    "urijs": "^1.17.1",
    "validator": "^5.2.0"
  },
  "optionalPeerDependencies": {
    "abavalidator": ">=2 <3",
    "bignumber.js": ">=7 <9",
    "creditcard": ">=0.0.1 <1.0.0",
    "google-libphonenumber": ">=1 <4",
    "iban": ">=0.0.6 <1.0.0",
    "isoc": ">=0.0.1 <1.0.0",
    "moment": ">=2 <3",
    "tin-validator": ">=1.0.0 <2.0.0",
    "uk-modulus-checking": "0.0.2",
    "urijs": ">=1 <2",
    "validator": ">=3 <6"
  },
  "engines": {
    "node": ">=8"
  }
}
